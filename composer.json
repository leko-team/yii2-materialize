{
  "name": "leko-team/yii2-materialize",
  "description": "This is the Materialize extension for Yii framework 2.0. It encapsulates Materialize components and plugins in terms of Yii widgets, and thus makes using Materialize components/plugins in Yii applications extremely easy.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "materialize",
    "material design",
    "materializecss"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Tuyakhov",
      "email": "atuyakhov@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "dogfalo/materialize": "0.97.6",
    "bower-asset/materialize": "0.97.*"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/leko-team/yii2-materialize.git"
    },
    {
        "type": "package",
        "package": {
            "name": "dogfalo/materialize",
            "version": "0.97.6",
            "dist": {
                "url": "https://github.com/Dogfalo/materialize/releases/download/v0.97.6/materialize-src-v0.97.6.zip",
                "type": "zip"
            }
        }
    }
  ],
  "autoload": {
    "psr-4": {
      "tuyakhov\\materialize\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
